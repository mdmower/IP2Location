{
  "name": "ip2ldb-reader",
  "version": "1.1.2",
  "description": "Reader for IP2Location databases",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "npm run lint && npm run compile",
    "compile": "tsc",
    "compile-testing": "tsc --build tsconfig-testing.json",
    "prepublishOnly": "npm run build",
    "cliTest": "node lib-testing/cli-testing.js"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdmower/ip2ldb-reader.git"
  },
  "keywords": [
    "ip2location",
    "geolocation"
  ],
  "author": "Matt Mower <mdmower@cmphys.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdmower/ip2ldb-reader/issues"
  },
  "homepage": "https://github.com/mdmower/ip2ldb-reader#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsdoc": "^30.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  }
}
